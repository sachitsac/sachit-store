{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["@mscharley"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-type-alias": "off",
    "@typescript-eslint/no-magic-numbers": "off"
  }
}
