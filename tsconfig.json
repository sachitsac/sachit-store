{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["es2016", "es2017", "es2018", "dom"],
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": ".",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true
  }
}
